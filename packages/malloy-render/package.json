{
  "name": "@malloydata/render",
  "version": "0.0.117",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/malloydata/malloy#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/malloydata/malloy"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "add_licence": "node ./src/bundle/bundle_license.js",
    "esbuild_bundle": "esbuild ./src/bundle/main.ts --bundle --sourcemap --outfile=./src/bundle/bundled_renderer.js --platform=browser --target=esnext '--define:process.env.NODE_DEBUG=\"development\"'",
    "bundle_renderer": "npm run esbuild_bundle && npm run add_licence",
    "lint": "eslint '**/*.ts{,x}'",
    "lint-fix": "eslint '**/*.ts{,x}' --fix",
    "test": "jest --config=../../jest.config.js",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@lit/context": "^1.1.0",
    "@malloydata/malloy": "^0.0.117",
    "@types/luxon": "^2.4.0",
    "lit": "^3.0.2",
    "lodash": "^4.17.20",
    "luxon": "^2.4.0",
    "ssf": "^0.11.2",
    "us-atlas": "^3.0.0",
    "vega": "^5.21.0",
    "vega-lite": "^5.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/blocks": "^7.5.0",
    "@storybook/html": "^7.5.0",
    "@storybook/html-vite": "^7.5.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/types": "^7.5.3",
    "esbuild": "^0.19.11",
    "storybook": "^7.5.0"
  }
}
